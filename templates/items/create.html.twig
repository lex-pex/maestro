{% extends 'layouts/app.html.twig' %}
{% block title %}{% if title is defined %}{{ title }}{% endif %}{% endblock %}
{% block main %}
<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">{{ title }}</div>
                <div class="card-body">
                    {{ form(form) }}
                </div>
                <div class="card-footer text-center">Maestro</div>
            </div>
        </div>
    </div>
</div>
{% endblock %}
